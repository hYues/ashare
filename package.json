{
  "name": "ashare",
  "displayName": "Ashare",
  "description": "Focus on the price of a single stock today.",
  "version": "0.0.5",
  "publisher": "HaoYue-VSCode",
  "author": "Haoyue",
  "icon": "public/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hYues/Ashare.git"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "ashare": {
          "type": "object",
          "default": {
            "stock": "SH000001",
            "picHeight": 578,
            "autoRefresh": false,
            "autoRefreshTime": 30000
          },
          "description": "setting"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
